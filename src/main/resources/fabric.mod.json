{
  "schemaVersion": 1,
  "id": "skija",
  "version": "${version}",
  "name": "Skija",
  "description": "Skija is an example Minecraft implementation that integrates the Skija graphics library to enhance rendering capabilities.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/skija/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.lyzev.skija.client.SkijaClient"
      }
    ]
  },
  "mixins": [
    "skija.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
